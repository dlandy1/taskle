<h1>New Todo</h1>
  <div class="col-md-4">
    <p>Create a title for your new to do list:</p>
  </div>
  <div class="col-md-10">
    <%= form_for @todo do |f| %>
      <div class="form-group">
        <%= f.label :title %>
        <%= f.text_field :title, class: 'form-control', placeholder: "Enter name of to do list" %>
      </div>
      <%= f.submit "Save", class: 'btn btn-success' %>
    <% end %>
  </div>
